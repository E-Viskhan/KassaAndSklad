@model IEnumerable<Uchet.Models.Shop>
@{
    ViewData["Title"] = "AllShops";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Все магазины заведенные в системе</h1>

<table class="table">
    <tr><td><b>Id магазина</b></td><td><b>Email владельца</b></td><td><b>Название магазина</b></td><td><b>Описание магазина</b></td></tr>
    @foreach (var item in Model)
    {
        <tr><td>@item.Id</td><td>@item.OwnerEmail</td><td>@item.Name</td><td>@item.Description</td></tr>
    }
</table>